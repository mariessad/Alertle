const mongoose = require("mongoose");
const { Schema } = mongoose;

const imageSchema = new Schema({
  name: {
    type: String,
  },
  desc: { type: String },
  cloudinary_id: {
    type: String,
  },
  location: { type: String },
  comment: { type: String },
});

module.exports = new mongoose.model("Image", imageSchema);
